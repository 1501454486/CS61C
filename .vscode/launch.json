{
  "version": "0.2.0",
    "configurations": [
      {
        "type": "venus",
        "request": "launch",
        "name": "Launch current file with all options",
        "program": "${file}",
        "stopOnEntry": true,
        "stopAtBreakpoints": true,
        "openViews": [
            "Robot",
            "LED Matrix",
            "Seven Segment Board"
        ],
        "ledMatrixSize": {
            "x": 10,
            "y": 10
        }
    },

    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "d:/code/CS61C/labs/lab02",
      "program": "d:/code/CS61C/labs/lab02/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}